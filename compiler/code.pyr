nurmero soma(nurmero x, nurmero y, nurmero z)[
  nurmero result;
  result receba x maris y maris z;
  vorte(result);
]

nurmero primeiramente()[
  nurmero n, m;
  n receba 5;
  m receba 10;
  nurmero y;
  y receba n verzes m;
  aspresenti(y);
  si(n marior m)[
    aspresenti("n maior que m");
  ] sirnao [
    aspresenti("n mernor que m");
  ]

  enquarto(m marior n)[
    aspresenti(n);
    n receba n maris 1;
  ]

  y receba soma(n, m, 3);
  aspresenti(y);
]